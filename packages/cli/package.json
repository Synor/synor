{
  "name": "@synor/cli",
  "description": "CLI for Synor - Database Schema Migration Tool",
  "version": "0.8.0",
  "author": "Munif Tanjim (https://muniftanjim.dev)",
  "bin": {
    "synor": "./bin/run"
  },
  "bugs": "https://github.com/Synor/synor/issues",
  "dependencies": {
    "@oclif/color": "^0.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1.14.0",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2",
    "cli-ux": "^5.4.5",
    "lodash.defaultsdeep": "^4.6.1",
    "oclif-plugin-completion": "^0.3.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/parser": "^3.8.5",
    "@synor/core": "^0.9.4",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/node": "^14.0.14",
    "@types/supports-color": "^5.3.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@synor/core": "^0.9.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Synor/synor/tree/master/packages/cli",
  "keywords": [
    "cli",
    "synor",
    "database",
    "migration",
    "schema",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "synor",
    "plugins": [
      "oclif-plugin-completion",
      "@oclif/plugin-help"
    ],
    "repositoryPrefix": "<%- repo %>/blob/@synor/cli@<%- version %>/packages/cli/<%- commandPath %>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Synor/synor",
    "directory": "packages/cli"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && npm run readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "echo NO TESTS",
    "readme": "oclif-dev readme",
    "preversion": "npm run readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
